<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#ffffff">

    <LinearLayout
        android:id="@+id/pub_ochatsdk_bottom_item1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:layout_marginRight="79dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/pub_ochatsdk_bottom_window_item_title1"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:textSize="16sp"
            android:textColor="#212121"
            android:text="北京市海淀区西小k淀区西淀区西淀区西" />

        <TextView
            android:id="@+id/pub_ochatsdk_bottom_window_item_title2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginLeft="28dp"
            android:text="13913787485"/>

    </LinearLayout>

    <TextView
        android:id="@+id/pub_ochatsdk_bottom_window_item_title3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/pub_ochatsdk_bottom_item1"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="12dp"
        android:layout_marginRight="79dp"
        android:text="北京市海淀区西小口路66号中关村东升科技园。北领地B-2号楼四层C-2"/>

    <CheckBox
        android:id="@+id/pub_ochatsdk_bottom_window_item_checkbox"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_centerVertical="true"
        style="@style/pub_ochatsdk_checkboxstyle"
        android:layout_alignParentRight="true"
        android:visibility="gone"/>
    
    <RadioButton
        android:id="@+id/pub_ochatsdk_bottom_window_item_radiobutton"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        style="@style/pub_ochatsdk_radiobuttonstyle" />

</RelativeLayout>